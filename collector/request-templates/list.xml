<foodquery>
    <type level="1"/>
    <selection>
        <atribute name="f_id"/>
        <atribute name="f_ori_name"/>
        <atribute name="langual"/>
        <atribute name="f_eng_name"/>
        <atribute name="f_origen"/>
        <atribute name="edible_portion"/>
    </selection>
    <condition>
        <cond1>
            <atribute1 name="f_ori_name"/>
        </cond1>
        <relation type="BEGINW"/>
        <cond3>${letter}</cond3>
    </condition>
    <condition>
        <cond1>
            <atribute1 name="f_origen"/>
        </cond1>
        <relation type="EQUAL"/>
        <cond3>BEDCA</cond3>
    </condition>
    <order ordtype="ASC">
        <atribute3 name="f_ori_name"/>
    </order>
</foodquery>